{
  "version": 2,
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node",
      "config": { "includeFiles": ["dist/**"] }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts"
    }
  ],
  "crons": [
    {
      "name": "every-5-minutes",
      "schedule": "*/5 * * * *",
      "command": "src/main.ts"
    }
  ]
}
